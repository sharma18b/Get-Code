{
    "name": "Get-Code",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "A simple extractor of code from youtube videos or images on the web",
    "browser_action": {
      "default_title": "Get-Code"
    },
    "icons": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "64": "images/64.png",
      "128": "images/128.png"
    },
    "background": {
      "persistent": false,
      "scripts": ["/background/background.js"]
    },
    "permissions": [
      "activeTab",
      "downloads"
    ]
  }